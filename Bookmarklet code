javascript:(function(){
    if (location.hostname.includes("[BLOCKED WEBSITE]")) {
        location.href = "https://buildsgordon.github.io/WebsiteBlockerBookmarklet/";
        return;
    }

    if (location.hostname.includes("youtube.com") && location.pathname.startsWith("/shorts")) {
        setTimeout(() => {
            if (location.hostname.includes("youtube.com") && location.pathname.startsWith("/shorts")) {
                location.href = "https://buildsgordon.github.io/WebsiteBlockerBookmarklet/";
            }
        }, 300000);
    }
})();
